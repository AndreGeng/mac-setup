{
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  },
  "npm.binPath":"~/.nvm/versions/node/v14.15.4/bin/npm",
  "explorer.openAction.strategy":"previousWindow",
  "explorer.width": 30,
  "explorer.position": "floating",
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "<space>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "u": "gotoParent",
    "df": "delete"
  }
}
